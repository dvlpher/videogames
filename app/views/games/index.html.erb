<h1>All Played Games</h1>

<ul>
  <% @games.each do |game|%>
    <li>
      <%= link_to game.title, game_path(game) %> -
      <%= game.rating %> -
      Difficulty Level: <%= game.difficulty_level %> -
        <ul>
          <% game.ordered_profiles.each do |p|%>
          <li><strong><%= p.name %></strong> |
            <%= p.last_updated%> |
            Owner: <%= p.user.username%>
          </li>
          <% end %>
        </ul>
    </li>
  <% end %>
</ul>